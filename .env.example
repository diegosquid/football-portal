NEXT_PUBLIC_SUPABASE_URL=your-supabase-url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key

# Unsplash API (fallback de imagens)
# Registrar em: https://unsplash.com/developers
UNSPLASH_ACCESS_KEY=your-unsplash-access-key

# Cloudflare R2 (hospedagem de imagens geradas)
CLOUDFLARE_ACCOUNT_ID=your-cloudflare-account-id
R2_ACCESS_KEY_ID=your-r2-access-key-id
R2_SECRET_ACCESS_KEY=your-r2-secret-access-key
R2_BUCKET_NAME=beiradocampo
R2_PUBLIC_URL=https://pub-xxx.r2.dev

# Google Gemini API (geracao de imagens com IA)
# Registrar em: https://aistudio.google.com/apikey
GEMINI_API_KEY=your-gemini-api-key
